div.pre-review
  ul.nav.nav-tabs
    li(role='presentaion')
      a(href='/comment/{{homeworkId}}') 评论
    li.active(role='presentation')
      a(href='/myComment') 我的评论

  div#my-comments
    p.text-center {{commentTitle}}
      a.back.btn.btn-link(href='/student') 返回
    div.col-md-10.col-md-offset-1.pre-comments(ng-repeat='(reviewer, comment) in myCommentList')

      // comments
      div.comment-panel
        div.col-md-10.comment
          p {{comment.comment}}
        div.pre-score.text-center
          p {{comment.score}}